/*********** MAIN STYLES *************/
a,p,b,span,h1,h2,h3,h4,h5,h6,body{
	font-family: 'Lato', sans-serif;
}
.o-layout--large>.o-layout__item{
	padding-left: 0;
}
footer#footer_mobile{
	display: none;
}
html {
    padding-bottom: 0 !important;
} 
.btn{
	transition: 0.3s;
	font-family: 'Lato', sans-serif;
	font-weight: 900;
	text-transform: uppercase;
	.main{
		width: 325px;
		max-width: 100%;
		height: 47px;
		display: flex;
		justify-content: center;
		align-items: center;
		background-color: #E65525;
		color: #fff;
		font-size: 15px;
		border: solid 1px #E65525;
		.background{
			background-color: #D14F24;
			border: solid 1px #D14F24;
			&:hover{
				background-color:#fff;
				color: #D14F24;
			}
		}
		&:hover{
			background-color:#fff;
			color: #E65525;
		}
	}
	.small{
		width: 185px;
		height: 47px;
		max-width: 100%;
		display: flex;
		justify-content: center;
		align-items: center;
		color: #fff;
		background-color: #E65525;
		border: solid 1px #E65525;
		font-size: 13px;
		&:hover{
			background-color:#fff;
			color: #E65525;
		}
	}
	.white{
		width: 170px;
		height: 47px;
		max-width: 100%;
		display: flex;
		justify-content: center;
		align-items: center;
		background-color: #fff;
		border: solid 1px #fff;
		color: transparent;
		font-size: 15px;
		&:hover{
			background-color:transparent;
			color: #fff;
		}
	}
	.details{
		width: 170px;
		height: 47px;
		max-width: 100%;
		display: flex;
		justify-content: center;
		align-items: center;
		background-color: #E65525;
		border: solid 1px #E65525;
		color: #fff;
		font-size: 15px;
		&:hover{
			background-color:#fff;
			color: #E65525;
		}
	}
}






/********** END MAIN STYLES ************/

iframe#preview-bar-iframe {
    display: none !important;
}




/********** STYLES HEADER ***********/
span.icon-fallback__text {
    display: none;
}
.mobile-draw--dark .mobile-search__input {
    border: unset;
    background-color: #f2f2f2;
    color: #989898;
    font-family: "Open Sans";
    font-size: 13px;
    font-weight: 400;
    line-height: 30px;
}

.mobile-draw--dark .mobile-search__input:focus {
    outline: none;
}
nav.log_in_menu_mobile {
    padding-top: 18px;
    padding-bottom: 18px;
    ul.list_menu_mobile {
	    list-style: none;
	    margin-bottom: 0;
	    margin-left: 11px;
	    a.item_mobile_list_inner {
		    color: #050708;
		    font-family: "Open Sans";
		    font-size: 14px;
		    font-weight: 400;
		}
	}	
}


.mobile-draw--dark .mobile-nav__link {
    padding-left: 11px;
    padding-right: 11px;
    color: #050708 !important;
    font-family: Lato;
    font-size: 16px;
    font-weight: 900;
    line-height: 42px;
    text-transform: initial;
}

.mobile-nav__item:last-child {border-bottom: solid 1px rgba(17,17,17,0.1);}

.mobile-nav__sub.mobile-nav__sub {
    margin-left: 20px;
}

.mobile-draw--dark .mobile-nav__sub__link {
    color: #050708;
    font-family: "Open Sans";
    font-size: 14px;
    font-weight: 400;
}
.mega_menu_shop{
	position: absolute;
    left: 0;
    right: 0;
    z-index: 11;
    background-color: #fff;
    border: solid 1px #e0e0e0;
    display: none;
    top: 86px;
}
header.header-new {
    border-bottom: solid 1px #eae4dc;
    position: relative;
    .inner_mega_menu {
	    padding-top: 27px;
	    padding-bottom: 50px;
	    display: flex;
	}

	.child_block_mega {
	    padding-left: 40px;
	    padding-top: 23px;
	    padding-bottom: 36px;
	    padding-right: 40px;
	    width: 33%;
	}

	.child_block_mega:last-child {background-color: #f7f7f7;}

	p.title_child_mega_text {
	    color: #050708;
	    font-size: 16px;
	    font-weight: 900;
	    line-height: 42px;
	    text-transform: initial;
	    margin-bottom: 0;
	}

	.delivery_mega {
	    border: 1px solid #dbdbdb;
	    margin-bottom: 21px;
	}

	.inner_sub_child_mega {
	    column-count: 2;
	}

	a.sub_child_link_inner {
	    color: #3c3c3c;
	    font-family: "Open Sans";
	    font-size: 15px;
	    font-weight: 400;
	    line-height: 30px;
	    text-transform: initial;
	    display: flex;
	}
}
.header_phone{
	display: none;
}
.header_inner {

    display: flex;
    justify-content: space-between;
    align-items: center;
    max-height: 86px;
    .primary-nav{
    	position: static;
    }
	.menu_section .header-trigger__link{
		&.js-cart-full span{
		    display: flex;
		    width: 15px;
		    height: 15px;
		    justify-content: center;
		    align-items: center;
		    top: 0;
		    left: 0;
		}
		 span {
		    position: absolute;
		    left: 3px;
		    top: 8px;
		    z-index: 10;
		    color: #ffffff;
		    font-family: "Open Sans";
		    font-size: 9px;
		    font-weight: 700;
		    display: none;
		}
	}

	.header-trigger__link.js-cart-full:after {
	    width: 15px;
	    height: 15px;
	    background-color: #e65525;
	    left: 0;
	}
	.menu_inner {
	    height: 86px;
	    display: flex;
	    align-items: center;
	}
    li.primary-nav__item {
    	height: 86px;
	    padding-top: 22px;
	    padding-bottom: 22px;
	    margin-right: 45px;
	    position: static;
	    &:hover{
	    	.mega_menu_shop{
		    	display: block;
		    }
	    }
	    &.primary-nav__item--sub:hover:after {
		    content: '';
		    height: 50px;
		    display: block;
		    background-image: url('https://cdn.shopify.com/s/files/1/0108/9572/0548/files/arrow_top.png?14132254627223488785');
		    background-repeat: no-repeat;
		    background-position: center top;
		}
	}
	.animsition-link {color: #454545;font-family: "Open Sans";font-size: 15px;font-weight: 400;line-height: 42px;text-transform: none;}
	.logo_section {
	    height: 86px;
	    display: flex;
		align-items: center;
	    img {
		    height: auto;
		    width: 100%;
		}
	}

	.menu_section {
	    display: flex;
		align-items: center;
		.header-trigger__link {
		    margin: 0 15px;
		    display: flex;
		    justify-content: center;
		    align-items: center;
		}
	}
}
.flex_inner_announcement {
    display: flex;
    justify-content: space-between;
    align-items: center;
    .left_announcement{
    	display: flex;
		.item_announcement{
			min-width: 80px;
			padding-left: 15px;
			padding-right: 15px;
			padding-top: 10px;
			padding-bottom: 10px;
			border-right: solid 1px #ca5124;
			img.item_inner{
				object-fit: contain;
			}
		}
    }
}
.inner_product_info {
    max-width: 1088px;
    margin: 0 auto;
    display: flex;
    padding-top: 23px;
    padding-bottom: 37px;
    border-bottom: 1px solid #e6e6e6;
    margin-bottom: 30px;
    .block_info_product {
	    margin-right: 30px;
	    &:last-child {
		    margin-right: 0;
		}

		.image_info {
		    display: flex;
		    justify-content: center;
		    align-items: center;
		    max-height: 105px;
		}

		h5.title_info_text {
		    color: #050708;
		    font-family: Lato;
		    font-size: 18px;
		    font-weight: 900;
		    line-height: 42px;
		    text-align: center;
		    text-transform: initial;
		    margin-bottom: 0;
		}

		p.content_info_text {
		    color: #332b29;
		    font-family: "Open Sans";
		    font-size: 15px;
		    font-weight: 400;
		    line-height: 30px;
		    text-align: center;
		}
	}	
}
.description_block_product {
    max-width: 1088px;
    margin: 0 auto;
    color: #292929;
    line-height: 30px;
    font-family: "Open Sans";
    font-size: 15px;
    font-weight: 400;
    margin-bottom: 31px;
    p{
		color: #292929;
	    line-height: 30px;
	    font-family: "Open Sans";
	    font-size: 15px;
	    font-weight: 400;
	    margin-bottom: 0;
    }
    i{
    	color: #292929;
		font-family: "Open Sans";
		font-size: 13px;
		font-style: italic;
		line-height: 30px;
    }
}



