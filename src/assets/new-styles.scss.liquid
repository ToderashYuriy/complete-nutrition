.contact-content-wrapper {
  max-width: 100%;
  display: flex;
  flex-direction: column;
  width: 1080px;
  margin: 0 auto;
}
.top-content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 285px;
}
h1.title, p.title-p {
  color: #fff;
  text-transform: none;
  margin: 0;
  padding: 0;
}
h1.title {
  font-size: 42px;
  text-align: left;
  font-family: Lato;
  font-weight: 900;
}
p.title-p {
  font-size: 18px;
  text-align: left;
  font-family: Lato;
  font-weight: 400;
}
.contact-main-content {
  display: flex;
  max-width: 100%;
  justify-content: space-between;
}
.form {
  display: flex;
  width: 60%;
}
.aside {
  display: flex;
  flex-direction: column;
  width: 30%;
  word-wrap: break-word;
}
.form, .aside {
  margin: 70px 0;
}
.form {
  display: flex;
  flex-direction: column;
}
.form-title, .aside-title, .aside-address-title {
  font-family: Lato;
  font-weight: 900;
  font-size: 22px;
  text-transform: none;
}
.aside-address-title {
  margin-bottom: 0;
}
.block-custom-title {
  font-family: Lato;
  font-weight: 900;
  font-size: 15px;
  text-transform: none;
  margin-bottom: 0;
}
.custom-block-wrapper {
  margin-top: 25px;
}
h4.page__contact-form__note.form-success {
  font-size: 18px;
  font-family: Lato;
  font-weight: 400;
}
input.c-btn {
  background-color: #E65525;
}
.contact_us_top {
  background-size: cover;
}
body.template-page .newsletter.newsletter--light {
  display: none;
}
div.aside p {
  line-height: 25px;
}
.error_page_top {
  height: 650px;
  background-size: cover;
}
.content-wrapper {
  display: flex;
  align-items: center;
  height: 650px;
}
.content {
  display: flex;
  justify-content: center;
  flex-direction: column;
  margin: 0 auto;
  width: 85%;
  height: 100%;
}
h1.error-page-title {
  font-size: 220px;
  line-height: 1;
  margin: 0;
  padding: 0;
  color: #fff;
  text-align: left;
  font-family: Lato;
  font-weight: 900;
}
h2.after-title-text {
  font-size: 42px;
  color: #fff;
  text-align: left;
  font-family: Lato;
  font-weight: 400;
  text-transform: initial;
}
a.button {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 225px;
  height: 47px;
  background-color: #E65525;
  font-size: 15px;
  color: #fff;
}
body.template-404 section.section.section--footer {
  display: none;
}
.privacy-background-img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-size: cover;
  height: 285px;
}
.privacy-content-wrapper {
  max-width: 100%;
  display: flex;
  flex-direction: column;
  width: 1080px;
  margin: 0 auto;
}
.privacy-title {
  font-size: 50px;
  color: #fff;
  font-family: Lato;
  font-weight: 900;
  text-transform: initial;
}
.privacy-main-content {
  display: flex;
  justify-content: space-between;
  margin-top: 65px;
}
.privacy-main-aside {
  display: flex;
  flex-direction: column;
  width: 15%;
}
.privacy-main {
  width: 80%;
}
.privacy-first-link, .privacy-second-link {
  font-size: 15px;
  text-align: left;
}
span.privacy-second-link {
  font-weight: 900;
  position: relative;
  width: max-content;
  max-width: 100%;
}
span.privacy-second-link:after {
  content: '';
  position: absolute;
  height: 2px;
  background-color: #ffd161;
  left: 0px;
  right: 0px;
  bottom: 0;
}
span.privacy-first-link {
  font-weight: 900;
  position: relative;
  width: max-content;
  max-width: 100%;
}
span.privacy-first-link:after {
  content: '';
  position: absolute;
  height: 2px;
  background-color: #ffd161;
  left: 0px;
  right: 0px;
  bottom: 0;
}
h2.initial {
  text-transform: initial;
  font-size: 18px;
  font-family: Lato;
  font-weight: 900;
  margin-top: 40px;
  margin-bottom: 25px;
  line-height: 22px;
}
h2.initial.first {
  margin-top: 0;
}
p.privacy-p {
  line-height: 30px;
  font-size: 15px;
  font-family: Lato;
  font-weight: 400;
}
ul.li-color-marker {
  margin-left: 0;
  li {
    list-style-type: none;
    position: relative;
    padding-left: 25px;
    &:before {
      color: #E65525;
      content: "";
      display: block;
      position: absolute;
      left: 0;
      width: 8px;
      height: 8px;
      background-color: #e65525;
      border-radius: 50%;
      top: 12px;
    }
  }
}
.career-background-img {
    display: flex;
    flex-direction: column;
    justify-content: center;
    background-size: cover;
    height: 285px;

  .career-content-wrapper {
      max-width: 100%;
      display: flex;
      flex-direction: column;
      width: 1080px;
      margin: 0 auto;

    .career-title {
        font-size: 50px;
        color: #fff;
        font-family: Lato;
        font-weight: 900;
        text-transform: initial;
    }
    h1.career-title {
      max-width: 40%;
    }
  }
}
.career-main {
    .career-content-wrapper {
      max-width: 100%;
      display: flex;
      flex-direction: column;
      width: 1080px;
      margin: 0 auto;
      margin-top: 55px;
      margin-bottom: 55px;

      p.career-title-text {
        font-size: 15px;
        line-height: 30px;
        text-align: center;

        a {
          text-decoration: underline;
        }
      }
      .table {
        display: flex;
        margin: 0 auto;
        margin-top: 65px;
        width: 95%;

        th {
          font-size: 16px;
          font-family: Lato;
          font-weight: 900;
          text-transform: initial;
          line-height: 25px;
          background-color: #fff;
          border-bottom: 1px solid #FAFAFA;
        }

        tbody {
          tr {
            height: 61px;
            font-family: Lato;
            font-weight: 400;

            &:nth-child(even) td {
              background-color: #fff;
            }
            &:nth-child(odd) td {
              background-color: #FAFAFA;
            }
            &:hover td {
              font-weight: 900;
              border-bottom: 2px solid #FFC331;
              min-height: 61px;
            }
            td {
              width: 50%;
            }
          }
        }
      }
    }
    
}
.career-join {
  .container {
    text-align: center;
    background-color: #F2EFEC;

    .career-join-wrapper {
      display: flex;
      align-items: center;
      justify-content: center;

      .join-content-wrapper {
        display: flex;
        flex-direction: column;
        align-items: center;
        margin: 0 auto;
        margin: 100px 0;
        width: 70%;

        h2.join-title {
          font-size: 32px;
          line-height: 32px;
          font-family: Lato;
          font-weight: 900;
          text-transform: initial;
          margin-bottom: 20px;
          
        }
        p.join-text {
          font-size: 15px;
          line-height: 25px;
          max-width: 85%;
        }

        .button-wrapper {
          display: flex;
          align-items: center;
          margin: 13px 0;

          .submit-button, .download-button {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            width: 255px;
            height: 47px;
            font-size: 15px;
            font-family: Lato;
            font-weight: 900;
          }
          .submit-button {
            background-color: #E65525;
            color: #fff;
            margin: 7px;

            &:hover {
              cursor: pointer;
              background-color: transparent;
              border: 1px solid #E65525;
              color: #E65525;
            }
          }
          .download-button {
            border: 1px solid #E65525;
            color: #E65525;
            margin: 7px;

            &:hover {
              background-color: #E65525;
              color: #fff;
              border: none;
            }
          }
        }
      }
    }
  }
}
.career-form-wrapper {
  margin: 70px 0;
  text-align: center;

  .career-form {
    display: flex;
    flex-direction: column;
    margin: 0 auto;
    width: 60%;

    .career-form-title {
      text-transform: initial;
      margin-bottom: 25px;
      font-family: Lato;
      font-weight: 900;
    }

    .upload-wrapper {
      display: flex;
      text-align: left;

      a.choose-button {
        display: flex;
        align-items: center;
        background-color: #FFC331;
        color: #fff;
        font-size: 13px;
        font-family: Lato;
        font-weight: 900;
        width: 173px;
        height: 47px;
        margin: 20px 0;
        padding-left: 20px;
        
        svg {
          margin-right: 10px;
        }
      }
      .upload-text {
        font-size: 13px;
        margin: 18px 0;
        padding: 0;
        margin-left: 15px;
        max-height: 47px;
      }
    }
    .career-form-submit {
      font-size: 15px;
      font-family: Lato;
      font-weight: 900;
      line-height: 18px;
      margin: 0;
      padding: 15px 0;
    }
    textarea.ContactFormMessage {
      margin-bottom: 0;
    }
  }
}
.career-gallery {
  .gallery-wrapper {
    display: flex;

    div.img-1, div.img-2, div.img-3, div.img-4, div.img-5 {
      width: 20%;
    }

    .img-1 img, .img-2 img, .img-3 img, .img-4 img, .img-5 img {
      width: 100%;
      height: 100%;
    }
  }
}



@media screen and (max-width: 1199px){
  .contact_us_top {
    background-size: cover;
  }
}
@media screen and (max-width: 1024px){
  .contact_us_top {
    background-size: cover;
  }
}
@media screen and (max-width: 991px){
  .contact_us_top {
    background-size: cover;
  }
}
@media screen and (max-width: 780px){
  .upload-wrapper {
    flex-direction: column;
  }
  .upload-text {
    margin: 0 !important;
  }
  a.choose-button {
    margin-bottom: 7px;
  }
}
@media screen and (max-width: 767px){
  h1.title, p.title-p {
    margin-left: 50px;
  }
  .contact-main-content {
    flex-direction: column;
  }
  .form, .aside {
    width: 80%;
    margin: 0 auto;
  }
  .form-title {
    margin-top: 30px;
  }
  h1.error-page-title {
    font-size: 160px;
    margin-bottom: 15px;
  }
  h2.after-title-text {
    font-size: 36px;
    color: #fff;
    text-align: left;
    margin-bottom: 20px;
  }
  .privacy-main-aside {
    display: none;
  }
  .privacy-main {
    width: 100%;
  }
  .privacy-main-content {
    margin: 35px 10px;
  }
}
@media screen and (max-width: 575px){
  .form, .aside {
    width: 95%;
  }
  .aside-title {
    margin-top: 35px;
    margin-bottom: 17px;
  }
  .aside-address-title {
    margin-top: 30px;
  }
  .main-content {
    margin-bottom: 20px;
  }
}
@media screen and (max-width: 479px){
  h1.title, p.title-p {
    margin-left: 20px;
  }
}
@media screen and (max-width: 425px){
  h1.title, p.title-p {
    margin-left: 10px;
  }
  div.aside {
    margin-bottom: 45px;
  }
}
@media screen and (max-width: 375px){
  .form, .aside {
    width: 100%;
  }
  .content-wrapper {
    height: 575px;
  }
  h1.error-page-title {
    margin-bottom: 25px;
  }
}
@media screen and (max-width: 320px){
	.form, .aside {
    width: 100%;
  }
  h1.error-page-title {
    font-size: 120px;
  }
  h2.after-title-text {
    font-size: 28px;
    color: #fff;
    text-align: left;
    margin-bottom: 20px;
  }
}