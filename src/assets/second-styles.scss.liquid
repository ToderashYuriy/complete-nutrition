/*PRODUCT ONE SECTION*/

	.home-product-one{
		flex-direction: column;
	}

	.product-bg-img-one{
		flex-basis: 100%;
		height: 400px;

	}

	.product-description-one{
		flex-basis: 100%;
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
		margin: 0 auto;
		height: 320px;
		padding-left: 40px;
		padding-right: 40px;
		order: 2;
	}

	.inner-product-description-one{
		
	h1{
		line-height: 20px;
		font-size: 22px;
	}
}

/*PRODUCT TWO SECTION */

	.home-product-two{
		flex-direction: column;
	}

	.product-bg-img-two{
		flex-basis: 100%;
		height: 400px;
		background-size: contain;
		background-repeat: no-repeat;
		text-align: center;
		background-position: center;
	}

	.product-description-two{
		flex-basis: 100%;
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
		margin: 0 auto;
		height: 350px;
		padding-left: 40px;
		padding-right: 40px;
	}

	.inner-product-description-two{

		h1{
			font-size: 22px;
			line-height: 20px;
		}
	}

/*	FIRST BENEFIT SECTION*/
	
	.benefit-one{
		-webkit-flex-direction: column;
		-moz-flex-direction: column;
		-ms-flex-direction: column;
		-o-flex-direction: column;
		flex-direction: column;
	}
	.home-first-benefit{
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
		flex-basis: 100%;
		height: 270px;
		margin: 17px;
	}

	.inner-benefit-one{
		bottom: 40px;
		left: 35px;
		right: 35px;
		max-width: 100%;

		h1{
			line-height: 30px;
			padding-bottom: 10px;
		}
	}

	.btn-home-first-benefit{
		display: none;
	}

	.home-second-benefit{
		height: 365px;

		h1{
			line-height: 36px;
			margin: 5px;
		}
		p{
			line-height: 26px;
		}
	}

	.home-second-benefit{
		margin: 17px;
		margin-top: 0;
	}


	/*	SECOND BENEFIT SECTION*/
	
	.benefit-two{
		-webkit-flex-direction: column;
		-moz-flex-direction: column;
		-ms-flex-direction: column;
		-o-flex-direction: column;
		flex-direction: column;
	}
	.home-third-benefit,.home-fourth-benefit{
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
		flex-basis: 100%;
		height: 270px;
		margin: 10px;
		margin: 17px;
		margin-top: 0;
	}

	.inner-benefit-third,.inner-benefit-fourth{
		bottom: 40px;
		left: 35px;
		right: 35px;

		h1{
			line-height: 30px;
			padding-bottom: 10px;
		}
	}

	.btn-home-third-benefit,.btn-home-fourth-benefit{
		display: none;
	}

	/*CONTACT*/

	.top-content{
		margin-top: 5%;

		h1{
			font-size: 30px;
			line-height: 62px;
		}
		p{
			font-size: 18px;
			line-height: 26px;
		}
	}

	/*TERMS*/

	.privacy-bg-img{		

		img{
			width: 100%;
			height: 300px;
			object-fit: cover;		}
		}

	.privacy-content{

		h1{
			font-size: 30px;
			line-height: 62px;
		}
		p{
			font-size: 18px;
			line-height: 26px;
		}
	}


	.term-btn-wrap{
		display: block;
		background-color: transparent;
		border: 1px solid #fff;
		text-align: center;
		width: 400px;
		max-width: 100%;

		h2{
			color: #fff;
			padding-top: 15px;
			font-size: 15px;
			letter-spacing: 0;
		}

		button:focus{
			outline: none;
		}
	}

	.wrap-links{
		display: none;
		position: absolute;
		width: 100%;
		height: 380px;
		background-color: #fff;
		top: -60px;
		left: 0;
		border-bottom: 1px solid grey;

		h1{
			border-bottom: 2px solid yellow;
			width: 100%;
		}
	}

	.close-btn{
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
		border-bottom: 2px solid grey;
		justify-content: space-between;
		border-bottom: 1px solid grey;

		h2{
			font-family: 'Open Sans', sans-serif;
			font-size: 15px;
			font-weight: 400;
			margin-top: 20px;
			letter-spacing: 0;
			text-transform: none;
			margin-left: 20px;
		}

		button{
			border: none;
			background-color: transparent;
			margin-right: 20px;
		}

		button:focus{
			outline: none;
		}
	}

	.privacy-aside{
		margin-left: 15px;
		margin-top: 30px;
		span{
			font-family: 'Open Sans', sans-serif;
			font-size: 15px;
			font-weight: bold;
			width: 30px;
			border-bottom: 1px solid #ffd161;
		}

		p{
			padding-top: 10px;
		}
	}

}

@media screen and (max-width: 576px){
	
	/*HOME TOP*/

	.home-top{
		height: 480px;
	}

	.header-home-top{
		padding-top: 90px;
		width: 260px;

		h1{
			font-size: 36px;
			line-height: 36px;
		}
	}

	.inner-story-description{

		p{
			padding-bottom: 10px;
		}

		h1{
			font-size: 22px;
			margin: 0;
		}
	}

	.inner-product-description-one,.inner-product-description-two  {
		p{
			padding-bottom: 10px;
		}
	}

	.btn-story,.btn-home-product-one,.btn-home-product-two{
		h2{
			font-size: 15px;
		}
	}

	.top-content{
		margin-top: 5%;

		h1{
			font-size: 20px;
			line-height: 42px;
		}
		p{
			font-size: 15px;
			line-height: 22px;
		}
	}
}






@media screen and (max-width: 1024px){

	/*SECOND BLOCK*/

	.box-img{
		flex-basis: 60%;
		margin-top: -70px;
	}

	/*ABOUT ADVANTAGES */

	.first-benefit,.second-benefit, .third-benefit{
		padding: 90px;
	}
}

@media screen and (max-width: 1024px){

	/*SECOND BLOCK*/

	.box-img{
		margin-top: -65px;
	}
}
@media screen and (max-width: 991px){

	/*SECOND BLOCK*/

	.box-img{
		margin-top: -65px;
	}

	/*ABOUT ADVANTAGES */

	.first-benefit,.second-benefit, .third-benefit{
		padding: 80px;
	}
}


@media screen and (max-width: 767px){

	/*ABOUT TOP*/

	.bg-top{
		height: 289px;
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
	    justify-content: flex-start;
	    align-items: center;
	}
	.text-top{
		width: 500px;
		max-width: 100%;

		h1{
			font-size: 36px;
			line-height: 36px;
		}
	}
	
	/*SECOND BLOCK*/

	.second-block{

		p{
			padding-left: 10px;
			padding-right: 10px;
		}
	}

	.box-img{
		flex-basis: 100%;
		text-align: center;
		margin-top: -130px;
	}

	.box-description{
		flex-basis: 100%;
		justify-content: center;
		padding: 30px;

		p{
			margin: 0;
			padding: 0;
		}
	}


	/*ABOUT ADVANTAGES */

	.about-advantages-first,.about-advantages-second,.about-advantages-third{
		display: -webkit-flex;
		display: -moz-flex;
		display: -ms-flex;
		display: -o-flex;
		display: flex;
		flex-direction: column;
		-webkit-flex-direction: column;
		-moz-flex-direction: column;
		-ms-flex-direction: column;
		-o-flex-direction: column;
	}

	.first-benefit,.third-benefit{
		order: 2;
	}

	.first-benefit,.second-benefit, .third-benefit{
		justify-content: center;
		padding: 30px;
	}

	.first-benefit-img,.second-benefit-img, .third-benefit-img{
		height: 100%;
		width: 100%;
		min-height: 480px;
	}

	/*VALUE*/

	.value-collection{
		-webkit-flex-direction: column;
		-moz-flex-direction: column;
		-ms-flex-direction: column;
		-o-flex-direction: column;
		flex-direction: column;
		justify-content: center;
		
		h1{
			padding-bottom: 10px;
		}

		img{
			padding-top: 0;
		}

		h2{
			text-transform: none;
			padding-top: 20px;
		}

		p{
			font-family: 'Open Sans', sans-serif;
			font-weight: 400;
			font-size: 15px;
			padding-bottom: 50px;
		}
	}

	.shop-now-info{
		h1{
			letter-spacing: 0;
			line-height: 1;
		}
	}
}



@media screen and (max-width: 576px){
	
	/*ABOUT SECOND*/

	.box-description{
		padding-top: 0px;
		padding-bottom: 40px;
	}

	.box-description{
		margin-top: 50px;
	}
}


